<?xml version="1.0"?>
<parameters>
  <formatVersion>2</formatVersion>
  <version>0.9.4</version>
  <cosmologyParametersMethod value="simple">
    <HubbleConstant value="73.0"/>
    <OmegaMatter value="0.25"/>
    <OmegaDarkEnergy value="0.75"/>
    <OmegaBaryon value="0.045"/>
  </cosmologyParametersMethod><cosmologyFunctionsMethod value="matterLambda"/>
  <criticalOverdensityMethod value="sphericalCollapseMatterLambda"/>
  <haloMassFunctionMethod value="tinker2008"/>
  <powerSpectrumPrimordialMethod value="powerLaw">
    <index value="1.000"/>
    <wavenumberReference value="1.000"/>
    <running value="0.000"/>
  </powerSpectrumPrimordialMethod>
  <cosmologicalMassVarianceMethod value="filteredPower">
    <sigma_8 value="0.9"/>
  </cosmologicalMassVarianceMethod><transferFunctionMethod value="eisensteinHu1999">
    <neutrinoNumberEffective value="3.04"/>
    <neutrinoMassSummed value="0.0"/>
  </transferFunctionMethod>

  <!-- File of tree timing data. -->
  <timePerTreeFitFileName value="treeTimingFit.xml"/>

  <!-- Parameter for the Behroozi et al. (2010) conditional stellar mass fitting function which give a good fit to the Li & White
       (2009) stellar mass function. -->
  <conditionalStellarMassFunctionBehrooziAlphaSatellite value="1.0"/>
  <conditionalStellarMassFunctionBehrooziLog10M1 value="12.1200"/>
  <conditionalStellarMassFunctionBehrooziLog10Mstar0 value="10.5160"/>
  <conditionalStellarMassFunctionBehrooziBeta value="0.430"/>
  <conditionalStellarMassFunctionBehrooziDelta value="0.5666"/>
  <conditionalStellarMassFunctionBehrooziGamma value="1.5300"/>
  <conditionalStellarMassFunctionBehrooziSigmaLogMstar value="0.2060"/>
  <conditionalStellarMassFunctionBehrooziBCut value="0.74400"/>
  <conditionalStellarMassFunctionBehrooziBSatellite value="8.0000"/>
  <conditionalStellarMassFunctionBehrooziBetaCut value="-0.1300"/>
  <conditionalStellarMassFunctionBehrooziBetaSatellite value="0.8590"/>

  <!-- The halo mass function sampling method to use -->
  <haloMassFunctionSamplingMethod value="stellarMassFunction"/>

</parameters>
