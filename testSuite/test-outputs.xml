<?xml version="1.0" encoding="UTF-8"?>
<!-- Defines a set of Galacticus models which explore available output options. -->
<!-- Andrew Benson                                                              -->
<!-- 26-Jan-2011                                                                -->
<parameterGrid>
  <emailReport>no</emailReport>
  <doAnalysis>no</doAnalysis>
  <modelRootDirectory>testSuite/outputs/test-outputs</modelRootDirectory>
  <baseParameters>testSuite/test-outputs-base.xml</baseParameters>
  <parameters>
    <parameter>
      <name>outputDensityContrastData</name>
      <value>true</value>
    </parameter>
    <parameter>
      <name>outputDensityContrastDataDarkOnly</name>
      <value>false</value>
      <value>true</value>
    </parameter>
    <parameter>
      <name>outputDensityContrastValues</name>
      <value>200</value>
      <value>200 500</value>
    </parameter>
  </parameters>
</parameterGrid>
