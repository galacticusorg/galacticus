    <outputHalfLightData value="true"/>
    <luminosityFilter value="RGO_I SDSS_g SDSS_r SDSS_i"/> 
    <luminosityType value="rest observed observed observed"/> 
    <luminosityRedshift value="0.0 0.1 0.1 0.1"/>
  </parameters>

  <!-- Output main branch -->
  <parameters>
