	<coolingAngularMomentumUseInteriorMean>
	  <value>true</value>
	  <value>false</value>
	</coolingAngularMomentumUseInteriorMean>
	</value>
	<value>darkMatter</value>
      </coolingMeanAngularMomentumFrom>
      </value>
    </coolingSpecificAngularMomentumMethod>
  </parameters>
  
  <!-- Subhalo mass loss rates. -->
