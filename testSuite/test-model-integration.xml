<?xml version="1.0" encoding="UTF-8"?>
<!-- Defines a set of Galacticus models which explore available output options. -->
<!-- Andrew Benson                                                              -->
<!-- 26-Jan-2011                                                                -->
<parameterGrid>
  <emailReport>no</emailReport>
  <doAnalysis>no</doAnalysis>
  <modelRootDirectory>testSuite/outputs/test-model-integration</modelRootDirectory>
  <baseParameters>parameters.xml</baseParameters>
  <launchMethod>pbs</launchMethod>
  <pbs>
    <ompThreads>12</ompThreads>
    <maxJobsInQueue>40</maxJobsInQueue>
  </pbs>

  <parameters>
  <formatVersion>2</formatVersion>
  <version>0.9.4</version>
    <!-- Set halo mass ranges -->
    <mergerTreeBuildHaloMassMaximum value="1.0e14"/>
    <mergerTreeBuildHaloMassMinimum value="1.0e10"/>
    <mergerTreeBuildTreesPerDecade value="150"/>
  </parameters>
  
</parameterGrid>
