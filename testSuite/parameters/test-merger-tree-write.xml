<?xml version="1.0" encoding="UTF-8"?>
<!-- Defines a set of Galacticus models which explore exporting of merger trees. -->
<!-- Andrew Benson                                                               -->
<!-- 12-October-2012                                                             -->
<parameterGrid>
  <emailReport>no</emailReport>
  <doAnalysis>no</doAnalysis>
  <modelRootDirectory>testSuite/outputs/test-merger-tree-write</modelRootDirectory>
  <baseParameters>testSuite/parameters/test-merger-tree-write-base.xml</baseParameters>
  <parameters>
    <formatVersion>2</formatVersion>
    <version>0.9.4</version>
    <mergerTreeConstructMethod>
      <value>build
      <mergerTreeOperatorMethod value="export">
	<outputFileName value="testSuite/outputs/test-merger-tree-write/exportedTreesGLC.hdf5"/>
	<exportFormat   value="galacticus"                                                    />
      </mergerTreeOperatorMethod>
      </value>
      <value>read
      <mergerTreeReadFileName value="testSuite/outputs/test-merger-tree-write/exportedTreesGLC.hdf5"/>
      <mergerTreeReadPresetSubhaloIndices value="false"/>
      <mergerTreeReadPresetOrbits value="false"/>
      <mergerTreeReadPresetMergerTimes value="false"/>
      <mergerTreeReadPresetMergerNodes value="false"/>
      <mergerTreeReadPresetSubhaloMasses value="false"/>
      <mergerTreeReadPresetPositions value="false"/>
      <mergerTreeReadPresetScaleRadii value="false"/>
      <mergerTreeReadPresetSpins value="false"/>
      </value>
      <value>read
      <mergerTreeReadFileName value="testSuite/data/mergerTrees/millenniumTestTreesGLC.hdf5"/>
      <allTreesExistAtFinalTime value="false"/>
      <cosmologyParametersMethod value="simple">
	<OmegaMatter value="0.25"/>
	<OmegaDarkEnergy value="0.75"/>
	<OmegaBaryon value="0.045"/>
	<HubbleConstant value="73.0"/>
      </cosmologyParametersMethod>
      <cosmologicalMassVarianceMethod value="filteredPower">
	<sigma_8 value="0.9"/>
      </cosmologicalMassVarianceMethod>
      <treeNodeMethodPosition value="preset"/>
      <mergerTreeOperatorMethod value="export">
	<outputFileName value="testSuite/outputs/test-merger-tree-write/exportedTreesIRATE.hdf5"/>
	<exportFormat   value="irate"                                                           />
      </mergerTreeOperatorMethod>
      <mergerTreeReadPresetOrbits value="false"/>
      <mergerTreeReadPresetMergerTimes value="false"/>
      <mergerTreeReadPresetSubhaloIndices value="false"/>
      <mergerTreeReadPresetMergerNodes value="false"/>
      <mergerTreeReadPresetSubhaloMasses value="false"/>
      <mergerTreeReadPresetPositions value="true"/>
      <mergerTreeReadPresetScaleRadii value="false"/>
      <mergerTreeReadPresetSpins value="false"/>
      </value>
    </mergerTreeConstructMethod>
  </parameters>
</parameterGrid>
