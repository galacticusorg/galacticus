      <timescaleOutflowFractionalMinimum value="0.001"/>
      <stellarFeedbackOutflowsMethod value="powerLaw">
        <velocityCharacteristic value="250.0"/>
        <exponent value="3.5"/>
      </stellarFeedbackOutflowsMethod>
    </stellarFeedbackOutflowsMethod>
  </nodeOperatorMethod>
</parameters>
