<?xml version="1.0"?>
<!-- Parameter file used for testing dark energy spherical collapse calculations in a flat, cosmological constant universe model. -->
<parameters>
  <formatVersion>2</formatVersion>
  <version>0.9.4</version>
  <cosmologyFunctionsMethod value="matterDarkEnergy">
    <darkEnergyEquationOfStateW0 value="-1.0"/>
  </cosmologyFunctionsMethod>
  <cosmologyParametersMethod value="simple">
    <OmegaMatter value="0.3"/>
    <OmegaDarkEnergy value="0.7"/>
    <OmegaBaryon value="0.0"/>
    <HubbleConstant value="70.0"/>
  </cosmologyParametersMethod><criticalOverdensityMethod value="sphericalCollapseMatterDE"/>
  <virialDensityContrastMethod value="sphericalCollapseMatterDE"/>
</parameters>
