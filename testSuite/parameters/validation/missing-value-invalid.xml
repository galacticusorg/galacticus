  <mergingTimescaleMultiplier value="0.75"/>
  <satelliteMergingMassMovementsMethod value="simple"/>
  <minorMergerGasMovesTo value="spheroid"/>
  <satelliteMergingRemnantSizeMethod value="Cole2000"/>
