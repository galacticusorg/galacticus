  </nodePropertyExtractor>
  
  <!-- Output file -->
  <outputFileName value="testSuite/outputs/testTreeExport.hdf5"/>


    <randomNumberGenerator value="GSL">
      <seed value="42"/>
    </randomNumberGenerator>
  
  <mergerTreeImporter>
    <fatalMismatches value="true"/>
    <reweightTrees value="false"/>
    <validateData value="false"/>
  </mergerTreeImporter>
  <mergerTreeInitializor/>

    <geometryLightcone value="square">
      <lengthReplication value="1000"/>
      <lengthHubbleExponent value="-1"/>
      <lengthUnitsInSI value="3.08567758e+22"/>
      <timeEvolvesAlongLightcone value="true"/>
      <angularSize value="0.16"/>
      <origin value=" 0.0             0.0             0.0"/>
