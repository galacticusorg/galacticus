  <xi:include href="progenitorMassFunctionResolution.xml" xpointer="xpointer(parameters/*)" xmlns:xi="http://www.w3.org/2001/XInclude"/>
  <xi:include href="simulation_MDPL_BigMDPL.xml"          xpointer="xpointer(parameters/*)" xmlns:xi="http://www.w3.org/2001/XInclude"/>
  <xi:include href="cosmology_MDPL.xml"                   xpointer="xpointer(parameters/*)" xmlns:xi="http://www.w3.org/2001/XInclude"/>
  <xi:include href="powerSpectrum_MDPL.xml"               xpointer="xpointer(parameters/*)" xmlns:xi="http://www.w3.org/2001/XInclude"/>
  <xi:include href="transferFunction_MDPL_CDM.xml"        xpointer="xpointer(parameters/*)" xmlns:xi="http://www.w3.org/2001/XInclude"/>
  <xi:include href="mergerTreeEvolution.xml"              xpointer="xpointer(parameters/*)" xmlns:xi="http://www.w3.org/2001/XInclude"/>

  <!-- Random number generator -->
  <randomNumberGenerator value="GSL">
    <seed value="9372"/>
  </randomNumberGenerator>

  <!-- Task control -->
  <evolveForestsWorkShare value="cyclic"/>
  
  <!-- Distribution of merger tree masses to build -->
  <mergerTreeBuildMasses value="sampledDistributionUniform">
    <!-- Minimum is 300 times the mass resolution -->
    <massTreeMinimum value="=[simulation/massParticle/resolutionX1]*300.0"/>
    <massTreeMaximum value="1.00e16"                          />
    <treesPerDecade  value="100"                            />
  </mergerTreeBuildMasses>
  <mergerTreeMassResolution value="fixed">
    <!-- 1/5th of 300 times the particle mass - such that our resolution is well below any mass scale that we care about -->
    <massResolution value="=[simulation/massParticle/resolutionX1]*[resolution/progenitorHaloParticleCountMinimum]/5.0"/>
  </mergerTreeMassResolution>
  
  <!-- Output options -->
  <outputFileName  value="BigMDPL.hdf5"/>
  <mergerTreeOutputter value="analyzer"/>
  <outputTimes value="list">
    <!-- Matched to BigMDPL snapshots -->
    <redshifts value="0.505343971097396 1.0 2.14465408805031 4.10204081632653 7.69565217391304"/>
  </outputTimes>
  <mergerTreeOperator value="regridTimes">
    <!-- Regrid the trees to massively speed up searching for parents etc. -->
    <outputTimes value="list">
      <!-- Matched to BigMDPL snapshots -->
      <redshifts value="0.0 0.505343971097396 1.0 2.14465408805031 4.10204081632653 7.69565217391304 9.0"/>
    </outputTimes>
  </mergerTreeOperator>
  <outputAnalysis value="multi">
    <!-- Minimum mass ratio for likelihood is the ratio of the BigMDPL 300 particle mass limit (3.48e10) to the minimum parent
         mass in the sample. -->
    <virialDensityContrastDefinition value="sphericalCollapseClsnlssMttrCsmlgclCnstnt"/>
    <outputAnalysis value="progenitorMassFunction">
      <fileName                     value="%DATASTATICPATH%/darkMatter/progenitorMassFunctions_BigMDPL.hdf5"/>
      <indexParent                  value="0"                                                                                        />
      <indexRedshift                value="0"                                                                                        />
      <massRatioLikelihoodMinimum   value="=[simulation/massParticle/resolutionX1]*[resolution/progenitorHaloParticleCountMinimum]/10.0^13.0"   />
      <massRatioLikelihoodMaximum   value="1.00"                                                                                     />
      <covarianceDiagonalize        value="true"                                                                                     />
      <covarianceTargetOnly         value="false"                                                                                    />
      <likelihoodInLog              value="false"                                                                                    />
      <rootVarianceTargetFractional value="=[progenitorMassFunctionParameters/rootVarianceTargetFractional]" 			     />
      <alwaysIsolatedOnly           value="true"                                                                                     />
      <redshiftParent               value="0.0"                                                                                      />
      <comment                      value="$13.00 &lt; \log_{10}(M_\mathrm{parent}/\mathrm{M}_\odot) \le 13.50$; $z=0.51$; BigMDPL halos"/>
      <label                        value="BigMDPLM0Z0"                                                                              />    									  <targetLabel                  value="Klypin et al. (2015)"                                                                     />
