<?xml version="1.0" encoding="UTF-8"?>
<simulations>
  <!-- Simulations to include into the dark matter halo mass function constraint pipeline -->
  <!-- Andrew Benson (01-December-2023) -->  

  <!-- For the MDPL simulations, "suites" are used to distinguish between different Rockstar versions.     -->
  <!-- This allows each to have their own detection efficiency, so that we allow for the possibility       -->
  <!-- that subtle differences between Rockstar versions lead to differences in detection efficiency.      -->
  <!-- Simulations are grouped into these suites based on the headers of their Rockstar merger tree files. -->
  <suite name="MDPL">
    <includeIsolationBias value="false"/>
    <includePerturbation  value="true" />
    <includeEnvironment   value="false"/>
    <limitMassMaximum     value="false"/>
    <group name="HugeMDPL" detectionEfficiencyClass="Rockstar1">
      <resolution name="resolutionX1">
	<simulation name="CDM">
	  <powerSpectrumClass value="HugeMDPL"/>
	  <label              value="HugeMDPL"/>
	  <transferFunction   value="CDM"     />
	</simulation>
	<haloMassFunction>
          <expansionFactors value="1.00000 0.67120 0.50320 0.33030 0.24770"/>
	</haloMassFunction>
      </resolution>
    </group>
    <group name="BigMDPL" detectionEfficiencyClass="Rockstar2">
      <resolution name="resolutionX1">
	<simulation name="CDM">
	  <powerSpectrumClass value="BigMDPL"/>
	  <label              value="BigMDPL"/>
	  <transferFunction   value="CDM"    />
	</simulation>
	<haloMassFunction>
          <expansionFactors value="1.00000 0.67040 0.50000 0.31800 0.25700"/>
	</haloMassFunction>
      </resolution>
    </group>
    <group name="MDPL2" detectionEfficiencyClass="Rockstar3">
      <resolution name="resolutionX1">
	<simulation name="CDM">
	  <powerSpectrumClass value="MDPL2"/>
	  <label              value="MDPL2"/>
	  <transferFunction   value="CDM"  />
	</simulation>
	<haloMassFunction>
          <expansionFactors value="1.00000 0.67120 0.50320 0.33030 0.24230"/>
	</haloMassFunction>
      </resolution>
    </group>
    <group name="SMDPL" detectionEfficiencyClass="Rockstar3">
      <resolution name="resolutionX1">
	<simulation name="CDM">
	  <powerSpectrumClass value="SMDPL"/>
	  <label              value="SMDPL"/>
	  <transferFunction   value="CDM"  />
	</simulation>
	<haloMassFunction>
          <expansionFactors value="1.00000 0.66430 0.50000 0.33100 0.24800"/>
	</haloMassFunction>
      </resolution>
    </group>
    <group name="VSMDPL" detectionEfficiencyClass="Rockstar4">
      <resolution name="resolutionX1">
	<simulation name="CDM">
	  <powerSpectrumClass value="VSMDPL"/>
	  <label              value="VSMDPL"/>
	  <transferFunction   value="CDM"   />
	</simulation>
	<haloMassFunction>
	  <expansionFactors value="1.00000 0.67110 0.50250 0.33000 0.24750"/>
	</haloMassFunction>
      </resolution>
    </group>
  </suite>
  
