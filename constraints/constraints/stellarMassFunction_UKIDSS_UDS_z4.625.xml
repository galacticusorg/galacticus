  <outputRedshift>4.625</outputRedshift>
  <haloMassMinimum>1.0e10</haloMassMinimum>
  <haloMassMaximum>1.0e15</haloMassMaximum>
  <analysis>constraints/scripts/stellarMassFunction_UKIDSS_UDS_z4.625.pl</analysis>
</constraint>
