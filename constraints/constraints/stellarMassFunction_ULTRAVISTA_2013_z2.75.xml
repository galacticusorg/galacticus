<!-- Defines a constraint to match to the ULTRAVISTA z~2.75 stellar mass functions. -->
<constraint>
  <name>ULTRAVISTA z~2.75 stellar mass functions</name>
  <label>ultravistaStellarMassFunctionZ2.75</label>
  <outputRedshift>2.50</outputRedshift>
  <outputRedshift>2.75</outputRedshift>
  <outputRedshift>3.00</outputRedshift>
  <haloMassResolution>5.0e9</haloMassResolution>
  <haloMassMinimum>2.0e10</haloMassMinimum>
  <haloMassMaximum>2.0e14</haloMassMaximum>
  <analysis>constraints/scripts/stellarMassFunction_ULTRAVISTA_z2.75.pl</analysis>
</constraint>
