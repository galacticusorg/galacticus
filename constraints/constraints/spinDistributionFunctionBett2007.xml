<!-- Defines a constraint to match to the Millennium Simulation spin distribution. -->
<constraint>
  <name>Millennium Simulation spin distribution function</name>
  <label>spinDistributionFunction</label>
  <outputRedshift>0.00</outputRedshift>
  <haloMassResolution>2.356e10</haloMassResolution>
  <haloMassMinimum>3.534e11</haloMassMinimum>
  <haloMassMaximum>1.0e15</haloMassMaximum>
  <analysis>constraints/scripts/spinDistributionFunctionBett2007.pl</analysis>
</constraint>
