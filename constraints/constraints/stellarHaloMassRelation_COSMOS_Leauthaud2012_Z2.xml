<!-- Defines a constraint to match to the z=0.48-0.74 COSMOS stellar mass vs. halo mass relation -->
<constraint>
  <name>COSMOS $$0.48$$--$$0.74$$ stellar mass vs. halo mass relation</name>
  <label>cosmosStellarHaloMassRelationZ0.61</label>
  <outputRedshift>0.61</outputRedshift>
  <haloMassResolution>1.0e10</haloMassResolution>
  <haloMassMinimum>1.0e11</haloMassMinimum>
  <haloMassMaximum>1.0e11</haloMassMaximum>
  <analysis>constraints/scripts/stellarHaloMassRelation_COSMOS_Leauthaud2012_Z2.pl</analysis>
</constraint>
